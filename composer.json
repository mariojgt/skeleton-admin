{
    "name": "mariojgt/skeleton-admin",
    "description": "Laravel master admin panel with both front and backend",
    "keywords": [
        "laravel"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mario",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/sanctum": "^3.0",
        "inertiajs/inertia-laravel": "^0.6.2",
        "tightenco/ziggy": "^1.5",
        "mariojgt/castle": "^2.0.1",
        "mariojgt/biscotto": "^1.0",
        "mariojgt/builder": "^1.0.5",
        "mariojgt/magnifier": "^1.0",
        "spatie/laravel-permission": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\SkeletonAdmin\\": "src"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\SkeletonAdmin\\SkeletonAdminProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
